{
    "manifest_version": 3,
    "name": "AI-gamen-kyouyu",
    "version": "0.1",
    "description": "Send browser screenshots to AI services (Antigravity, ChatGPT, Gemini, Claude).",
    "key": "",
    "permissions": [
        "activeTab",
        "scripting",
        "nativeMessaging"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "AI Screen Share"
    }
}